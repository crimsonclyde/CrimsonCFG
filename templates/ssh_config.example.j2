# Example SSH Configuration
# Copy this file to ssh_config.j2 and customize for your environment

Host server1
    HostName 192.168.1.100
    User user
    Port 22
    IdentityFile ~/.ssh/id_rsa

Host server2
    HostName 192.168.1.101
    User user
    Port 22
    IdentityFile ~/.ssh/id_rsa

Host github
    HostName github.com
    User git
    Port 22
    IdentityFile ~/.ssh/id_rsa 